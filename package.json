{
  "name": "turbo_app",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "servers": "turbo run dev --filter=worker --filter=server",
    "my-app": "turbo run dev --filter=worker --filter=server --filter=web-app"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.3.1",
    "typescript": "5.5.4"
  },
  "packageManager": "pnpm@8.15.6",
  "engines": {
    "node": ">=18"
  }
}
